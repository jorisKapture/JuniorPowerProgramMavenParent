<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="SurveyDetail" table="SURVEYDETAILS">
      <meta attribute="class-description">
         This class contains the surveydetail detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="score" column="score" type="int" />
      <many-to-one name="survey" class="Survey" fetch="select" >
            <column name="survey_id" not-null="true"  />
      </many-to-one>
      <many-to-one name="skill" class="Skill" fetch="select" >
            <column name="skill_id" not-null="true" />
      </many-to-one>      
      
   </class>
</hibernate-mapping>